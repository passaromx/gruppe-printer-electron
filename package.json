{
  "name": "gruppe-printer",
  "version": "v1.2.1-Beta",
  "author": "Passaro",
  "description": "System for Zebra Printers",
  "private": true,
  "repository": "https://github.com/passaromx/gruppe-printer-electron",
  "binary": {
    "module_name": "node_printer",
    "module_path": "./lib",
    "host": "https://github.com/tojocky/node-printer/releases/download/",
    "remote_path": "{version}"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build -w",
    "electron:serve": "vue-cli-service electron:serve",
    "createCert": "electron-builder create-self-signed-cert -p passaromx",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@vuetify/vuex-cognito-module": "^0.4.0",
    "axios": "^0.18.0",
    "electron-log": "^2.2.17",
    "electron-pdf-window": "^1.0.12",
    "electron-updater": "^4.0.7",
    "form-data": "^2.5.0",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^4.0.5",
    "moment": "^2.23.0",
    "node-pre-gyp": "^0.12.0",
    "printer": "git+https://github.com/tojocky/node-printer.git",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "shortid": "^2.2.14",
    "vee-validate": "^2.1.5",
    "vue": "^2.5.17",
    "vue-i18n": "^8.7.0",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^3.6.95",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-node-gyp": "git://github.com/tojocky/grunt-node-gyp.git",
    "grunt-nw-gyp": "git://github.com/tojocky/grunt-nw-gyp.git",
    "lint-staged": "^7.2.2",
    "nodeunit": "*",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-electron-builder": "^1.0.0-rc.10",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "background.js"
}
